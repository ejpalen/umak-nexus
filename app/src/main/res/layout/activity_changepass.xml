<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Changepass">

    <Button
        android:id="@+id/getStarted_btn3"
        android:layout_width="353dp"
        android:layout_height="53dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="442dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="164dp"
        android:background="@drawable/bg_btn_m"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="@font/syne_bold"
        android:foregroundTint="#0057FF"
        android:outlineProvider="background"
        android:text="CANCEL"
        android:textColor="#0057FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/idImgBtn"
        android:layout_width="23dp"
        android:layout_height="18dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="364dp"
        android:layout_marginBottom="607dp"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:src="@drawable/back_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="321dp"
        android:layout_marginBottom="605dp"
        android:clickable="false"
        android:fontFamily="@font/syne"
        android:text="Settings"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/changepass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="109dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="515dp"
        android:fontFamily="@font/syne_bold"
        android:text="Change Password"
        android:textAlignment="textEnd"
        android:textColor="#0057FF"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textOldPass"
        android:layout_width="216dp"
        android:layout_height="16dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="479dp"
        android:fontFamily="@font/syne_semibold"
        android:text=" OLD PASSWORD"
        android:textAlignment="inherit"
        android:textColor="#808080"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="358dp"
        android:layout_height="41dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="430dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg_btn_item" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="358dp"
        android:layout_height="41dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="285dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="333dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg_btn_item" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="216dp"
        android:layout_height="16dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="263dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="380dp"
        android:fontFamily="@font/syne_semibold"
        android:text=" NEW PASSWORD"
        android:textAlignment="inherit"
        android:textColor="#808080"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="342dp"
        android:layout_height="39dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="189dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="431dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/syne_semibold"
        android:inputType="text"
        android:text="Enter Old Password"
        android:textColor="#808080"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editNewPass"
        android:layout_width="342dp"
        android:layout_height="39dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="285dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="335dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/syne_semibold"
        android:inputType="text"
        android:text="Enter New Password"
        android:textColor="#808080"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/getStarted_btn2"
        android:layout_width="353dp"
        android:layout_height="53dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="373dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="233dp"
        android:background="@drawable/white_bg_round_corners"
        android:backgroundTint="#0057FF"
        android:fontFamily="@font/syne_bold"
        android:text="UPDATE PASSWORD"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="715dp"
        android:background="#f2f2f2"
        android:elevation="2dp"
        android:padding="5dp"
        app:itemIconSize="25dp"
        app:itemIconTint="@drawable/item_selector"
        app:itemRippleColor="@android:color/transparent"
        app:labelVisibilityMode="unlabeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:menu="@menu/bottom_profile" />


</androidx.constraintlayout.widget.ConstraintLayout>